{
  "name": "webpack-cli-multi",
  "version": "1.0.2",
  "description": "a simple webpack config cli for multi apps suooprt vue & react",
  "main": "views/*/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "echo \"Error: you need plus view's name after 'npm run dev-',just like 'npm run dev-pc'\" && exit 1",
    "dev-mobile": "cross-env NODE_ENV=devcelopMobile PLATFORM=web webpack-dev-server",
    "dev-pc": "cross-env NODE_ENV=devcelopPc PLATFORM=web webpack-dev-server",
    "build-mobile": "cross-env NODE_ENV=productionMobile PLATFORM=web webpack",
    "build-pc": "cross-env NODE_ENV=productionPc PLATFORM=web webpack",
    "build": "npm run build-mobile && npm run build-pc"
  },
  "keywords": [],
  "author": "watanabeyu",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "image-webpack-loader": "^3.3.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "markup-inline-loader": "^0.2.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.1",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^11.3.4",
